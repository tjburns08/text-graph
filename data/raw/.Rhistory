dat <- readLines('bible_cleaned.txt')
nvec <- length(dat)
breaks <- which(! nzchar(dat))
nbreaks <- length(breaks)
if (breaks[nbreaks] < nvec) {
breaks <- c(breaks, nvec + 1L)
nbreaks <- nbreaks + 1L
}
if (nbreaks > 0L) {
dat <- mapply(function(a,b) paste(dat[a:b], collapse = " "),
c(1L, 1L + breaks[-nbreaks]),
breaks - 1L)
}
breaks
